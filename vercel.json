{
  "rewrites": [
    {
      "source": "/api-fund/:path*",
      "destination": "https://fundgz.1234567.com.cn/:path*"
    },
    {
      "source": "/api-sina/(.*)",
      "destination": "/api/sina?path=$1"
    }
  ],
  "headers": [
    {
      "source": "/api-fund/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    },
    {
      "source": "/api-sina/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ]
}
